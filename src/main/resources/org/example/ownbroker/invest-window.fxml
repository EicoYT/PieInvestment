<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="15.0" xmlns:fx="http://javafx.com/fxml" fx:controller="org.example.ownbroker.mainmenu.MainMenuMethods">

    <!-- Welcome Label -->
    <Label text="Welcome to the Invest Window" style="-fx-font-size: 24px;"/>

    <!-- Stock Price Chart -->
    <LineChart fx:id="stockChart" title="Stock Price" animated="true" prefWidth="600" prefHeight="400">
        <xAxis>
            <NumberAxis label="Time"/>
        </xAxis>
        <yAxis>
            <NumberAxis label="Price"/>
        </yAxis>
    </LineChart>

</VBox>
